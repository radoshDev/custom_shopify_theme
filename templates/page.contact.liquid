{% form 'contact' %}
  {{ form.errors | default_errors }}
  <h1>Contacts</h1>


  <div class="email">
    <label for="email">Email</label>
    <input
      type="email"
      name="contact[email]"
      id="email" />
  </div>
  <div class="email">
    <label for="test">Test</label>
    <input
      type="text"
      name="contact[test]"
      id="test" />
  </div>

  <div class="message">
    <label for="message">Message</label>
    <textarea name="contact[body]" id="message"></textarea>
  </div>

  <div class="submit">
    <input type="submit" value="Send" />
  </div>
{% endform %}